<?xml version="1.0" encoding="UTF-8" ?>
<phpdocumentor configVersion="3.0">
    <paths>
        <output>docs/dev/phpdoc</output>
        <cache>cache/phpdocumentor</cache>
    </paths>
    <version number="3.0.0">
        <api format="php">
            <source dsn=".">
                <path>htdocs</path>
            </source>
            <ignore hidden="true" symlinks="true">
                <path>vendor/**/*</path>
                <path>includes/**/*</path>
            </ignore>
            <extensions>
                <extension>php</extension>
            </extensions>
            <include-source>true</include-source>
            <markers>
                <marker>TODO</marker>
                <marker>FIXME</marker>
            </markers>
        </api>
        <guide format="rst">
            <source dsn=".">
                <path>support/docs</path>
            </source>
            <output>docs</output>
        </guide>
    </version>
    <setting name="string" value="string"/>
</phpdocumentor>